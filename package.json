{
  "name": "runmymodel-desktop",
  "version": "1.0.0",
  "description": "RunMyModel Desktop - Run LLMs locally with Ollama",
  "main": "index.js",
  "scripts": {
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "build:windows": "tauri build --target x86_64-pc-windows-msvc",
    "build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "build:macos": "tauri build --target x86_64-apple-darwin",
    "build:macos-arm": "tauri build --target aarch64-apple-darwin",
    "build:all": "npm run build:windows && npm run build:linux && npm run build:macos",
    "build:linux-complete": "./build_linux.sh",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "ollama",
    "desktop",
    "ai",
    "chat",
    "runmymodel"
  ],
  "author": "RunMyModel.org",
  "license": "MIT",
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.4",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.23",
    "pkg": "^5.8.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0"
  },
  "dependencies": {
    "@huggingface/hub": "^2.6.10",
    "@huggingface/inference": "^4.11.0",
    "@tauri-apps/api": "^2.8.0",
    "@types/react-syntax-highlighter": "^15.5.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "tailwind-merge": "^1.14.0",
    "zustand": "^4.4.0"
  }
}
