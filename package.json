{
  "name": "runmymodel-desktop",
  "version": "1.0.0",
  "description": "RunMyModel Desktop - Run LLMs locally with Hugging Face",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:cpp": "mkdir -p build && cd build && cmake .. && make",
    "build:cpp-debug": "mkdir -p build && cd build && cmake -DCMAKE_BUILD_TYPE=Debug .. && make",
    "build:cpp-release": "mkdir -p build && cd build && cmake -DCMAKE_BUILD_TYPE=Release .. && make",
    "run:cpp": "npm run build:cpp && ./build/RunMyModelDesktop",
    "clean:cpp": "rm -rf build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "huggingface",
    "desktop",
    "ai",
    "chat",
    "runmymodel",
    "cpp",
    "qt"
  ],
  "author": "RunMyModel.org",
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.23",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0"
  },
  "dependencies": {
    "@huggingface/hub": "^2.6.10",
    "@huggingface/inference": "^4.11.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react-syntax-highlighter": "^15.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "tailwind-merge": "^1.14.0",
    "zustand": "^4.4.0"
  }
}
